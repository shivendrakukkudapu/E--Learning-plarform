<h2>Welcome back,<span><%= name %></span>!!</h2>
<section id="statistics">
  <div class="row">
    <div class="learningprogress col-md-4">
<img src="lprog.PNG" alt="progress-image">
<div class="containerdashboard-graph">
	  <div class="graphbar">
	    <ul class="numbers">
		    <li><span>100%</span></li>
			<li><span>50%</span></li>
			<li><span>0%</span></li>
		</ul>
		<ul class="bars">
			<%  modules.forEach(function(post){ %>
                <li><div class="bar" data-percentage="<%= (post.quizresult/10*100) %>"></div><span><%= post.module %></span></li>
             <% }) %>  
		</ul>
	  </div>
	</div>
	<script type="text/javascript">
	$(function(){
	    $('.bars li .bar').each(function(key, bar){
		    var percentage = $(this).data('percentage');
			$(this).animate({
			 'height': percentage + '%'
			},1000);
		});
	});
	</script>
    </div>
    <div class="timeoflearning col-md-4">
<img src="timel.PNG" alt="time-image">
<div class="timee1head">
<p>Time spent</p> 
</div>
<div class="timeel">
  <p><%= hour %>hr:<%= minute %>min:<%= second %>s</p>
</div>
    </div>
    <div class="rank col-md-4">
<img src="rank.PNG" alt="rank-image">
<div class="rankee">
  <p><%= ranksss %></p>
   <p> Out of <%= outoff %></p>
</div>
    </div>
  </div>

</section>
<section id="subjects">
  <div class="row">
    <div class="physics col-lg-3 col-md-6 col-sm-12">
<a href="#">
