<%- include("partials/header1"); -%>


<table class="table table-hover">
 <thead>
   <tr>
     <th>Teacher name</th>
	 <th>Description</th>
	 <th>Time</th>
	 <th>Submit your response</th>
   </tr>
 </thead>
 <tbody>
    <% teachers.forEach(function(postssec){ %>
	<tr>
	<form action="/counseling" method="post">
     <td><%= postssec.username %></td>
	 <td><input class="form-control" type="text" placeholder="Description" name="description"></td>
	 <td><input class="form-control" type="text" placeholder="Time" value="<%= postssec.time %>" readonly><%= postssec.time %></td>
	 <td><button type="submit" class="btn btn-primary" name="studentsubmit" value="<%= postssec.username %>">Submit</button></td>
	 </form>
	</tr>
	<% }) %>
 </tbody>  
</table>

<%- include("partials/footer1"); -%>