<%- include("partials/header3"); -%>

<table class="table table-hover">
 <thead>
   <tr>
     <th>Student name</th>
     <th>Teacher name</th>
	 <th>Description</th>
	 <th>Time</th>
	 <th>Submit your response</th>
   </tr>
 </thead>
 <tbody>
    <% counselingss.forEach(function(postssecd){ %>
	<tr>
	<form action="/teacher/counselling" method="post">
	 <td><%= postssecd.studentusername %></td>
     <td><%= postssecd.teacherusername %></td>
	 <td><input class="form-control" type="text" placeholder="Description" name="description" value="<%=postssecd.description%>" readonly></td>
	 <td><input class="form-control" type="text" placeholder="Time" name="time"></td>
	 <td><button type="submit" class="btn btn-primary" name="studentsub" value="<%= postssecd.studentusername %>">Submit</button></td>
	 </form>
	</tr>
	<% }) %>
 </tbody>  
</table>

<%- include("partials/footer1"); -%>