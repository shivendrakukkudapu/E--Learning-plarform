<%- include("partials/header3"); -%>
<div class="dashboardheading">
<h1>Welcome <%= username %></h1>
</div>


<table class="table table-hover">
 <thead>
   <tr>
     <th>Username</th>
	 <th>Points</th>
	 <th>Dashboard</th>
   </tr>
 </thead>
 <tbody>
    <% students.forEach(function(postss){ %>
	<tr>
     <td><%= postss.username %></td>
	 <td><%= postss.points %></td>
	 <td><a href="/progress/<%=postss._id%>">Dashboard</a></td>
	</tr>
	<% }) %>
 </tbody>  
</table>


<%- include("partials/footer1"); -%>
