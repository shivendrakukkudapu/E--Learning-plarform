<%- include("partials/header4"); -%>

<table class="table table-hover">
 <thead>
   <tr>
     <th>Student name</th>
     <th>Teacher name</th>
	 <th>Description</th>
	 <th>Time</th>
   </tr>
 </thead>
 <tbody>
    <% counselingsss.forEach(function(postsscd){ %>
	<tr>
	 <td><%= postsscd.studentusername %></td>
     <td><%= postsscd.teacherusername %></td>
	 <td><input class="form-control" type="text" placeholder="Description" name="description" value="<%=postsscd.description%>" readonly></td>
	 <td><input class="form-control" type="text" placeholder="Time" name="time" value="<%=postsscd.time%>" readonly></td>
	</tr>
	<% }) %>
 </tbody>  
</table>

<%- include("partials/footer1"); -%>